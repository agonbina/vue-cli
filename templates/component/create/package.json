{
    "name": "{{ name }}",
    "private": true,
    "version": "0.1.0",
    "description": "{{ description }}",
    "main": "index.js",
    "dependencies": {
<% if(components.length !== 0) { %>
    <% _.forEach(components, function(component) { %>
        "{{ component }}": "git+ssh://git@gitlab.com:1aim/{{ component }}.git",
    <% }); %>
<% } %>

<% if(mixins.length !== 0) { %>
    <% _.forEach(mixins, function(mixin) { %>
        "{{ mixin }}": "git+ssh://git@gitlab.com:1aim/{{ mixin }}.git",
    <% }); %>
<% } %>
    },
    "author": "Agon Bina <agon_bina@hotmail.com>"
}
